{
  "name": "@rs-platon/ui-kit",
  "version": "0.0.6",
  "private": false,
  "main": "dist/ui-kit.umd.js",
  "module": "dist/ui-kit.es.js",
  "exports": {
    ".": {
      "import": "./dist/ui-kit.es.js",
      "require": "./dist/ui-kit.umd.js"
    },
    "./dist/style.css": {
      "default": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "deploy-storybook": "npm run build-storybook && gh-pages -d storybook-static",
    "build-storybook": "storybook build",
    "build-windows-npm": "vite build && copy src\\assets\\main.css dist\\style.css",
    "build-mac-npm": "vite build && cp src/assets/main.css dist/style.css"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@storybook/addon-a11y": "^9.0.15",
    "@storybook/addon-docs": "^9.0.15",
    "@storybook/addon-onboarding": "^9.0.15",
    "@storybook/addon-vitest": "^9.0.15",
    "@storybook/vue3-vite": "^9.0.15",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "gh-pages": "^6.3.0",
    "playwright": "^1.53.2",
    "sass": "^1.89.2",
    "storybook": "^9.0.15",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.2.4"
  }
